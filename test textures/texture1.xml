<texturegraph width="256" height="256" seed="1" supersampling="1">
	<block type="save file" id="0">
		<input id="7" slot="0"/>
		<parameter name="path" type="string" value="texture.png"/>
	</block>
	<block type="perlin noise" id="1">
		<parameter name="amplitude" type="int" value="127"/>
		<parameter name="frequency" type="int" value="6"/>
		<parameter name="max iterations" type="int" value="-1"/>
		<parameter name="interpolation" type="int" value="1"/>
		<parameter name="smooth" type="bool" value="1"/>
	</block>
	<block type="circle transform" id="2">
		<input id="1" slot="0"/>
		<parameter name="radius" type="int" value="5"/>
		<parameter name="jumps" type="int" value="1"/>
	</block>
	<block type="voronoi diagram" id="3">
		<parameter name="type" type="int" value="1"/>
		<parameter name="metric" type="int" value="0"/>
		<parameter name="point place" type="int" value="0"/>
		<parameter name="width" type="double" value="0.75"/>
		<parameter name="point positions" type="string" value=""/>
		<parameter name="number of points" type="int" value="15"/>
		<parameter name="initial x" type="double" value="0.5"/>
		<parameter name="initial y" type="double" value="0.5"/>
		<parameter name="initial angle" type="int" value="90"/>
	</block>
	<block type="replace colors" id="4">
		<input id="8" slot="0"/>
		<input id="2" slot="1"/>
		<input id="3" slot="2"/>
		<parameter name="color 1 red" type="int" value="255"/>
		<parameter name="color 1 green" type="int" value="255"/>
		<parameter name="color 1 blue" type="int" value="255"/>
		<parameter name="color 2 red" type="int" value="0"/>
		<parameter name="color 2 green" type="int" value="0"/>
		<parameter name="color 2 blue" type="int" value="0"/>
		<parameter name="color 3 red" type="int" value="170"/>
		<parameter name="color 3 green" type="int" value="170"/>
		<parameter name="color 3 blue" type="int" value="170"/>
		<parameter name="color 4 red" type="int" value="85"/>
		<parameter name="color 4 green" type="int" value="85"/>
		<parameter name="color 4 blue" type="int" value="85"/>
	</block>
	<block type="tile" id="5">
		<input id="4" slot="0"/>
		<parameter name="times" type="int" value="2"/>
	</block>
	<block type="normal map" id="6">
		<input id="5" slot="0"/>
		<parameter name="neighbourhood size" type="int" value="5"/>
	</block>
	<block type="light" id="7">
		<input id="6" slot="0"/>
		<parameter name="ambient red" type="int" value="5"/>
		<parameter name="ambient green" type="int" value="0"/>
		<parameter name="ambient blue" type="int" value="10"/>
		<parameter name="diffuse red" type="int" value="150"/>
		<parameter name="diffuse green" type="int" value="100"/>
		<parameter name="diffuse blue" type="int" value="100"/>
		<parameter name="specular red" type="int" value="255"/>
		<parameter name="specular green" type="int" value="245"/>
		<parameter name="specular blue" type="int" value="240"/>
		<parameter name="reflection curve" type="int" value="1"/>
		<parameter name="viewer height" type="double" value="1"/>
		<parameter name="phong exponent" type="double" value="2"/>
		<parameter name="direction vector x" type="double" value="0.5"/>
		<parameter name="direction vector y" type="double" value="0.5"/>
	</block>
	<block type="square mosaic" id="8">
		<parameter name="side 1" type="string" value="0.1 0.1 0.2 0.15 0.3 0.20 0.4 0.15 0.5 0.0"/>
		<parameter name="side 2" type="string" value="0.5 0.1"/>
		<parameter name="side 3" type="string" value="0.19 0.0 0.2 0.2 0.4 0.2 0.41 0.0"/>
		<parameter name="side 4" type="string" value=""/>
		<parameter name="transformation 1" type="int" value="1"/>
		<parameter name="transformation 2" type="int" value="0"/>
		<parameter name="transformation 3" type="int" value="1"/>
		<parameter name="transformation 4" type="int" value="0"/>
		<parameter name="tiles x" type="int" value="2"/>
		<parameter name="tiles y" type="int" value="2"/>
		<parameter name="fill type" type="int" value="2"/>
		<parameter name="fill colors" type="string" value="255 0 0 255"/>
	</block>
</texturegraph>

