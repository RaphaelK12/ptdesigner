<texturegraph width="128" height="100" seed="15" supersampling="3">
	<block type="file save" id="0">
		<input id="4" slot="0"/>
		<parameter name="path" type="string" value="texture.png"/>
	</block>
	<block type="perlin noise" id="1" seed="10">
		<parameter name="amplitude" type="int" value="127"/>
		<parameter name="frequency" type="int" value="6"/>
		<parameter name="max iterations" type="int" value="-1"/>
		<parameter name="interpolation" type="int" value="1"/>
		<parameter name="smooth" type="bool" value="1"/>
	</block>
	<block type="bump noise" id="2">
		<parameter name="bump size from" type="double" value="0.7"/>
		<parameter name="bump size to" type="double" value="0.01"/>
		<parameter name="quantity" type="int" value="1"/>
		<parameter name="alter amplitude" type="bool" value="0"/>
	</block>
	<block type="color fill" id="3">
		<parameter name="red" type="int" value="255"/>
		<parameter name="green" type="int" value="255"/>
		<parameter name="blue" type="int" value="255"/>
	</block>
	<block type="mix channels" id="4">
		<input id="1" slot="0"/>
		<input id="2" slot="1"/>
		<input id="3" slot="2"/>
	</block>
</texturegraph>

