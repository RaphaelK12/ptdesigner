<!DOCTYPE ptgraph>
<texturegraph width="512" height="512" seed="1" supersampling="1">
	<block type="save file" id="0">
		<input id="1" slot="0"/>
		<parameter name="path" type="string" value="texture.png"/>
	</block>
	<block type="mix" id="1">
		<input id="2" slot="0"/>
		<input id="3" slot="1"/>
		<parameter name="percentage" type="int" value="50"/>
		<parameter name="method" type="int" value="1"/>
	</block>
	<block type="load file" id="2">
		<parameter name="path" type="string" value="image.png"/>
		<parameter name="interpolation" type="int" value="1"/>
	</block>
	<block type="load file" id="3">
		<parameter name="path" type="string" value="image2.png"/>
		<parameter name="interpolation" type="int" value="1"/>
	</block>
</texturegraph>

